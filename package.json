{
  "name": "dont-break",
  "description": "Checks if the current version of your package would break dependent projects",
  "version": "0.3.3",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "dont-break": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/dont-break/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test",
        "npm version"
      ],
      "post-commit": "npm version"
    }
  },
  "contributors": [],
  "dependencies": {
    "chdir-promise": "0.2.1",
    "check-more-types": "1.7.3",
    "check-types": "1.4.0",
    "commander": "2.8.1",
    "lazy-ass": "0.5.8",
    "lodash": "3.9.3",
    "npm-registry": "0.1.13",
    "npm-utils": "0.1.4",
    "q": "2.0.3",
    "quote": "0.4.0",
    "shelljs": "0.5.1",
    "strip-json-comments": "1.0.2",
    "update-notifier": "0.5.0"
  },
  "devDependencies": {
    "commitizen": "2.4.1",
    "cz-conventional-changelog": "1.1.4",
    "eslint-rules": "0.4.3",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "0.11.2",
    "grunt-contrib-watch": "0.6.1",
    "grunt-deps-ok": "0.8.0",
    "grunt-eslint": "15.0.0",
    "grunt-filenames": "0.4.0",
    "grunt-jscs": "1.8.0",
    "grunt-nice-package": "0.9.2",
    "jshint-summary": "0.4.0",
    "matchdep": "0.3.0",
    "pre-git": "1.0.0"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "homepage": "https://github.com/bahmutov/dont-break",
  "keywords": [
    "update",
    "test",
    "dependency",
    "semver",
    "check"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:bahmutov/dont-break.git"
  },
  "scripts": {
    "build": "grunt",
    "test": "grunt && npm run show-help",
    "show-help": "node index.js --help",
    "test-empty": "node src/test/run-dont-break.js"
  }
}
