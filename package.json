{
  "name": "dont-break",
  "description": "Checks if the current version of your package would break dependent projects",
  "version": "0.4.2",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "dont-break": "./bin/dont-break"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/dont-break/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run mocha",
        "npm test"
      ],
      "post-commit": "npm version",
      "pre-push": [
        "npm run size"
      ],
      "post-merge": []
    }
  },
  "contributors": [],
  "files": [
    "bin",
    "index.js",
    "src/*.js",
    "!src/*-spec.js"
  ],
  "dependencies": {
    "chdir-promise": "0.2.1",
    "check-more-types": "2.22.0",
    "commander": "2.9.0",
    "debug": "2.2.0",
    "hr": "0.1.3",
    "lazy-ass": "1.5.0",
    "lodash": "3.10.1",
    "npm-registry": "0.1.13",
    "npm-utils": "0.3.0",
    "os-tmpdir": "1.0.1",
    "q": "2.0.3",
    "quote": "0.4.0",
    "shelljs": "0.5.3",
    "strip-json-comments": "2.0.0",
    "top-dependents": "1.0.0",
    "update-notifier": "0.5.0"
  },
  "devDependencies": {
    "eslint-rules": "0.4.3",
    "git-issues": "1.2.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-watch": "0.6.1",
    "grunt-deps-ok": "0.8.0",
    "grunt-eslint": "15.0.0",
    "grunt-filenames": "0.4.0",
    "grunt-jscs": "2.3.0",
    "grunt-nice-package": "0.10.2",
    "jshint-summary": "0.4.0",
    "matchdep": "1.0.1",
    "mocha": "2.3.3",
    "pre-git": "3.10.0"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "homepage": "https://github.com/bahmutov/dont-break",
  "keywords": [
    "check",
    "dependency",
    "semver",
    "test",
    "update"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:bahmutov/dont-break.git"
  },
  "scripts": {
    "build": "grunt",
    "issues": "git-issues",
    "mocha": "mocha src/**/*-spec.js",
    "show-help": "./bin/dont-break --help",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "grunt && npm run show-help",
    "test-empty": "node src/test/run-dont-break.js"
  }
}
